#include <iostream>
using namespace std;
int main() 
{
int k = 1, n, sum = 0;
cin >> n;
while (k < n)
    {
    if (n % k == 0) sum += k;
    k ++;
    }
if (sum == n)
{
    k = 1;
    while (k < n)
    {
    if(n % k == 0);
    k++;
    }
    cout << "совершенное";
}
else cout << "НЕ совершенное";
return 0;
}
